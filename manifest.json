{
  "name": "Code It Up!",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Programing for the masses.",
  "browser_action": {
    "default_icon": "icons/16-full-color.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-1.7.2.js", "content_script.js"],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "omnibox": {
    "keyword" : "quickcode"
  },
  "icons": {
    "16": "icons/16-full-color.png",
    "48": "icons/48-full-color.png",
    "128": "icons/128-full-color.png"
  },
  "permissions": [
    "http://api.flickr.com/",
    "tabs" // used to communicate
  ]
}
